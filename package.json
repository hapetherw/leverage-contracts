{
  "name": "leverage-contracts",
  "version": "0.0.1",
  "description": "A set of files and configs for getting started using Defi saver contracts",
  "scripts": {
    "compile": "npx hardhat compile",
    "clean": "npx hardhat clean && rm -rf dist cache types",
    "deploy": "cmd/run_deploy",
    "test": "cmd/run_test",
    "commit": "cz",
    "verify": "cmd/run_verify",
    "lint": "./node_modules/.bin/solhint -f table contracts/**/*.sol",
    "pretty": "./node_modules/.bin/prettier --write contracts/**/*.sol",
    "coverage": "npx hardhat coverage",
    "size:check": "npx hardhat size-contracts",
    "fetch:price": "node scripts/utils/price-tracker.js"
  },
  "devDependencies": {
    "@defisaver/sdk": "^1.0.51",
    "@defisaver/tokens": "^1.5.0",
    "@leveragecom/sdk": "^0.1.5",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.7",
    "@nomiclabs/hardhat-ethers": "^2.0.5",
    "@nomiclabs/hardhat-etherscan": "^3.1.2",
    "@nomiclabs/hardhat-waffle": "^2.0.3",
    "@openzeppelin/contracts": "^4.8.0",
    "@openzeppelin/hardhat-upgrades": "^1.19.0",
    "@typechain/ethers-v5": "^10.0.0",
    "@typechain/hardhat": "^6.0.0",
    "@types/chai": "^4.3.1",
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.35",
    "chai": "^4.3.6",
    "dotenv": "^10.0.0",
    "ethereum-waffle": "^3.4.4",
    "ethers": "^5.5.4",
    "hardhat": "^2.15.0",
    "hardhat-contract-sizer": "^2.10.0",
    "hardhat-gas-reporter": "^1.0.9",
    "solidity-coverage": "^0.7.20",
    "ts-generator": "^0.1.1",
    "ts-node": "^10.8.0",
    "typechain": "^8.0.0",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "@uniswap/sdk-core": "^4.0.7",
    "@uniswap/smart-order-router": "^3.16.33",
    "coingecko-api": "^1.0.10",
    "commander": "^11.0.0",
    "decimal.js": "^10.4.3",
    "fs-extra": "^10.0.0"
  }
}
